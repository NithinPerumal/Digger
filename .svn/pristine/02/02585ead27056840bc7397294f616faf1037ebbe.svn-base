public abstract class AbstractBlock {

	protected int x;
	protected int y;

	
	
	public AbstractBlock(int x,int y){
		this.x = x;
		this.y = y;
		
	}
	
	
	public void setLocation(int x, int y){
		this.x = x;
		this.y = y;
	}
	
	public abstract void draw(Graphics2D g);
	
	public abstract String returnType();


}
