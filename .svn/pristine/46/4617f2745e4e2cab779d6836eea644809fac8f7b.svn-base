public class Block extends AbstractBlock {


	private Color color=Color.BLACK;
	
	public Block(int x, int y) {
		super(x, y);
	}

	
	
	@Override
	public void draw(Graphics2D g){
		Rectangle temp = new Rectangle(this.x*40, this.y*40,40,40);
		g.setColor(this.color);
		g.fill(temp);
	}
	

	
}
